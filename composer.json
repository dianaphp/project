{
    "name": "dianaphp/project",
    "description": "a brand new diana project",
    "keywords": [
        "diana",
        "framework",
        "php"
    ],
    "minimum-stability": "dev",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "phpunit"
        ],
        "post-update-cmd": [
            "phpunit"
        ]
    },
    "authors": [
        {
            "name": "Antonio Ianzano",
            "email": "ianzanoan@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../framework",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../rendering",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": ">=8.3",
        "dianaphp/framework": "dev-main",
        "dianaphp/rendering": "dev-main",
        "dianaphp/routing": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2@dev"
    }
}